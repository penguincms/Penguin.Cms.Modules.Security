<Project Sdk="Microsoft.NET.Sdk.Razor">
  <PropertyGroup>
    <TargetFramework>netcoreapp3.1</TargetFramework>
    <AddRazorSupportForMvc>true</AddRazorSupportForMvc>
    <Nullable>enable</Nullable>
    <GenerateEmbeddedFilesManifest>true</GenerateEmbeddedFilesManifest>
    <PackageProjectUrl>https://github.com/penguincms/Penguin.Cms.Modules.Security</PackageProjectUrl>
    <Version>1.0.0</Version>
    <Description>Not Available</Description>
    <AssemblyName>Penguin.Cms.Modules.Security</AssemblyName>
    <RootNamespace>Penguin.Cms.Modules.Security</RootNamespace>
    <DocumentationFile>Penguin.Cms.Modules.Security.xml</DocumentationFile>
  </PropertyGroup>
  <ItemGroup>
    <Content Remove="wwwroot\**\*" />
    <PackageReference Include="Microsoft.Extensions.FileProviders.Embedded" Version="3.1.1" />
    <PackageReference Include="Penguin.Authentication.OWA" Version="1.0.3-T9QUIQ4" />
    <PackageReference Include="Penguin.Security" Version="1.1.8-T9QUIQ4" />
    <PackageReference Include="Penguin.Web.Configuration" Version="1.0.1-T9QUIQ4" />
    <PackageReference Include="Penguin.Cms.Web.Security" Version="3.1.1-T9QUIQ4" />
    <PackageReference Include="Penguin.Cms.Security.Repositories" Version="3.4.1-T9QUIQ4" />
    <PackageReference Include="Penguin.Cms.Modules.Dynamic" Version="1.0.1-T9R24Z6" />
  </ItemGroup>
  <ItemGroup>
    <EmbeddedResource Include="wwwroot\**\*" />
  </ItemGroup>
  <ItemGroup>
    <FrameworkReference Include="Microsoft.AspNetCore.App" />
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="System.DirectoryServices" Version="4.7.0" />
    <PackageReference Include="System.DirectoryServices.AccountManagement" Version="4.7.0" />
  </ItemGroup>
  <Import Project="Penguin.Projects.Shared\Penguin.Projects.Shared.csproj" />
</Project>