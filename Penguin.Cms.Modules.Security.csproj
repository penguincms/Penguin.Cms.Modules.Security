<Project Sdk="Microsoft.NET.Sdk.Razor">

  <PropertyGroup>
    <TargetFramework>netcoreapp3.0</TargetFramework>
    <AddRazorSupportForMvc>true</AddRazorSupportForMvc>
    <Nullable>enable</Nullable>
    <GenerateEmbeddedFilesManifest>true</GenerateEmbeddedFilesManifest>
  </PropertyGroup>

  <ItemGroup>
    <Content Remove="wwwroot\**\*" />
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Include="wwwroot\**\*" />
  </ItemGroup>
  
  <ItemGroup>
    <FrameworkReference Include="Microsoft.AspNetCore.App" />
  </ItemGroup>
  
  <ItemGroup>
    <PackageReference Include="Microsoft.Extensions.FileProviders.Embedded" Version="3.0.0" />
    <PackageReference Include="Penguin.Authentication.OWA" Version="1.0.2" />
    <PackageReference Include="Penguin.Cms.Security.Repositories" Version="3.4.0" />
    <PackageReference Include="Penguin.Cms.Web.Security" Version="3.1.0" />
    <PackageReference Include="Penguin.Messaging.Application" Version="2.1.0" />
    <PackageReference Include="Penguin.Security" Version="1.1.7" />
    <PackageReference Include="Penguin.Shared.Objects" Version="2.0.1" />
    <PackageReference Include="Penguin.Web.Configuration" Version="1.0.0" />
    <PackageReference Include="System.DirectoryServices" Version="4.6.0" />
    <PackageReference Include="System.DirectoryServices.AccountManagement" Version="4.6.0" />
  </ItemGroup>
  
  <ItemGroup>
    <ProjectReference Include="..\Penguin.Cms.Modules.Dynamic\Penguin.Cms.Modules.Dynamic.csproj" />
  </ItemGroup>


</Project>
